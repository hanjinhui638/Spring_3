<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="qnaMapper">
  <select id="qnaList" resultType="com.jh.s3.model.board.QnaVO" parameterType="com.jh.s3.util.RowMaker">
  
  	select*from
  		(select Q.*,rownum R from)  <!--  Q.* Q table 모든것 가지고 오기  -->
  				(select* from qna order by num desc) Q)
  		where R between #{startRow} and #{lastRow}			
  
  </select>
  
  <insert id="qnaInsert" parameterType="com.jh.s3.model.board.QnaVO">
  		insert into qna
  		values(board_seq.nextval,#{title}, #{writer}, #{contents}, sysdate, 0,0,0,0)
  
  </insert>		

  
 
  </mapper>